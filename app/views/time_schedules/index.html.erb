<p id="notice"><%= notice %></p>

<h1>Time Schedules</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_schedules.each do |time_schedule| %>
      <tr>
        <td><%= link_to 'Show', time_schedule %></td>
        <td><%= link_to 'Edit', edit_time_schedule_path(time_schedule) %></td>
        <td><%= link_to 'Destroy', time_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time Schedule', new_time_schedule_path %>
